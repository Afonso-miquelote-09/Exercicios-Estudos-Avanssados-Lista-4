#include <stdio.h>

int main() {
    int vetor[10];  // Declaração do vetor de 10 elementos
    int i;

    // Entrada dos elementos do vetor
    printf("Digite 10 elementos:\n");
    for (i = 0; i < 10; i++) {
        printf("Elemento %d: ", i + 1);
        scanf("%d", &vetor[i]);
    }

    // Apresentando os elementos do vetor
    printf("\nOs elementos inseridos são:\n");
    for (i = 0; i < 10; i++) {
        printf("Elemento %d: %d\n", i + 1, vetor[i]);
    }

    return 0;
}
